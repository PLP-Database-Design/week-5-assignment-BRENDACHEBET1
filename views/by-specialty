            <tr>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Provider Specialty</th>
            </tr>
        </thead>
        <tbody>
            <% if (results.length === 0) { %>
                <tr>
                    <td colspan="3">No providers found.</td>
                </tr>
